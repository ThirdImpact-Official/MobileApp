{
  "name": "escapegamenextdoor",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "start-high-limit": "cross-env NODE_OPTIONS=--max-old-space-size=4096 expo start",
    "windows-start": "cross-env CHOKIDAR_USEPOLLING=true expo start",
    "start:fast": "cross-env EXPO_NO_BUNDLER_SPLIT=true EXPO_USE_FAST_RESOLVER=1 expo start --max-workers=4 ",
    "start:prod": "NODE_ENV=production EXPO_NO_DEV_CLIENT=1 expo start --minify",
    "start:win": "set CHOKIDAR_USEPOLLING=true&& set EXPO_METRO_CACHE_DIR=%TEMP%\\metro-cache&& expo start",
    "clean:metro": "rm -rf node_modules/.cache/metro $TMPDIR/metro-*",
    "clean:full": "npm run clean:metro && rm -rf .expo .expo-shared",
    "profile:metro": "EXPO_DEBUG=1 expo start --profile",
    "debug:ram": "node -e \"console.log('Free RAM:', require('os').freemem() / 1024 / 1024, 'MB')\"",
    "build:android": "EXPO_NO_DEV_CLIENT=1 expo run:android --release",
    "build:ios": "EXPO_NO_DEV_CLIENT=1 expo run:ios --release",
    "build:web": "NODE_OPTIONS=--max-old-space-size=4096 expo export -p web"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@expo/vector-icons": "^14.0.2",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.0.2",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "axios": "^1.9.0",
    "expo": "~52.0.46",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.20",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.2",
    "jwt-decode": "^4.0.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^18.3.0",
    "cross-env": "^7.0.3",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
